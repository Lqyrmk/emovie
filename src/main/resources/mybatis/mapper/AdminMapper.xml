<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lqyrmk.emovie.mapper.AdminMapper">

    <!--    查询管理员-->
    <select id="getAdminByName" resultType="com.lqyrmk.emovie.entity.Admin">
        select * from admin
        where name = #{name};
    </select>

    <!--    增加管理员-->
    <insert id="addAdmin">
        insert into admin(name, password)
        values(#{name},  #{password});
    </insert>


</mapper>